-register long __sc7 __asm__ ("r7") = (long) arg6;
-__asm__ __volatile__ ("trapa	%1" \
-	: "=r" (__sc0) \
-	: "r" (__sc0), "r" (__sc2), "r" (__sc3), "r" (__sc4), "r" (__sc5), \
-	  "r" (__sc6), "r" (__sc7));
-__syscall_return(long,__sc0);
-}
