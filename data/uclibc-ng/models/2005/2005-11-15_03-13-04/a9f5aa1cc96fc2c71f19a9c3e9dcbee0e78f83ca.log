index 7a0c48733..8389f74e5 100644
--- a/Rules.mak
+++ b/Rules.mak
@@ -333,9 +333,13 @@ endif
 ifeq ($(UCLIBC_HAS_THREADS),y)
 ifeq ($(UCLIBC_HAS_THREADS_NATIVE),y)
 	PTNAME := nptl
+else
+ifeq ($(LINUXTHREADS_OLD),y)
+	PTNAME := linuxthreads.old
 else
 	PTNAME := linuxthreads
 endif
+endif
